/*
 * -------------------------------------------------
 *  Nextflow config file for running tests
 * -------------------------------------------------
 * Defines bundled input files and everything required
 * to run a full pipeline test. Use as follows:
 *   nextflow run nf-core/atacseq -profile test_full,<docker/singularity>
 */

params {
  config_profile_name = 'Test 2 profile'
  config_profile_description = 'Test 2 dataset to check pipeline function'

  // Input data
  input = 'https://raw.githubusercontent.com/jonathanbader/atacseq/main/input_sources/2.csv'

  // Genome references
  genome = 'hg19'
}
